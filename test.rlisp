#enable-preprocessor

; compose : (a -> b) (b -> c) -> (a -> c)
; Produces a new function that represents the composition of the two specified
; functions.
define : compose f g
    lambda : x
        f : g x

define : square x
    * x x

; map* : (a -> b) list[a] -> list[b]
define : map* f xs
    cond 
        (empty? xs) xs
        else
            cons
                f : head xs
                map* f : tail xs

define : displn str
    begin
        display str
        newline

define : id* x
    . x